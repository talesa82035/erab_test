		SIF GETBIT(ARG:1, 2)
			LOCALS += "アナルそば/"
		SIF GETBIT(ARG:1, 3)
			LOCALS += "陰茎/"
		STRLENS LOCALS
		SUBSTRING LOCALS, 0, RESULT - 1
		RETURNF RESULTS
	CASE フレーバー素質_ふたなり時_玉ありなし
		IF ARG:1 == フレーバー素質_ふたなり時_玉なし
			RETURNF "玉なし"
		ELSEIF ARG:1 == フレーバー素質_ふたなり時_玉あり
			RETURNF "玉あり"
		ENDIF
ENDSELECT

