		LOCAL = 1500
		PRINTW ：道具
	;指
	ELSEIF ARG:2 == 3
		LOCAL = 1000
		PRINTW ：指
	;触手
	ELSEIF ARG:2 == 4
		LOCAL = 2000
		PRINTW ：触手
	ENDIF
	LOCAL = LOCAL * PAIN_CHECK_V(ARG) / 1000
	CALL SOURCE_CALC_苦痛(ARG, ARG:1, LOCAL)
	;不潔
	;貞操観念
	IF TALENT:ARG:貞操 > 0
		;非恋慕
		IF !TALENT:ARG:恋慕
			CALL SOURCE_CALC_不潔(ARG, ARG:1, 1000)
		;恋愛対象以外の相手
		ELSEIF ARG:1 != PLAYER
			CALL SOURCE_CALC_不潔(ARG, ARG:1, 2000)
		;チンコじゃない
		ELSEIF ARG:2 != 1
			CALL SOURCE_CALC_不潔(ARG, ARG:1, 500)
		ENDIF
	ELSEIF TALENT:ARG:貞操 == 0
		;非恋慕
		IF !TALENT:ARG:恋慕
			CALL SOURCE_CALC_不潔(ARG, ARG:1, 500)
		;恋愛対象以外の相手
		ELSEIF ARG:1 != PLAYER
			CALL SOURCE_CALC_不潔(ARG, ARG:1, 1000)
		;チンコじゃない
		ELSEIF ARG:2 != 1
			CALL SOURCE_CALC_不潔(ARG, ARG:1, 250)
		ENDIF
	ENDIF
	;陥落素質による処理
	;MASTERがTARGETの膣に挿入している
	IF ARG:1 == MASTER
		IF TALENT:ARG:貞操 > 0
			;非恋慕
			IF !TALENT:ARG:恋慕
				CALL SOURCE_CALC_不潔(ARG, ARG:1, 1000)
			;チンコじゃない
			ELSEIF ARG:2 != 1
				CALL SOURCE_CALC_不潔(ARG, ARG:1, 500)
			ENDIF
		ELSEIF TALENT:ARG:貞操 == 0
			;非恋慕
			IF !TALENT:ARG:恋慕
				CALL SOURCE_CALC_不潔(ARG, ARG:1, 500)
			;チンコじゃない
			ELSEIF ARG:2 != 1
				CALL SOURCE_CALC_不潔(ARG, ARG:1, 250)
			ENDIF
		ENDIF
		IF ARG:2 == 1
			;親愛かつ相手が主人
			IF TALENT:ARG:恋慕 > 1
				PRINTFORML %CALLNAME:ARG%は%CALLNAME:MASTER%が初めての相手である事にこの上ない幸せを感じているようだ……
				CALL SOURCE_CALC_歓楽(ARG, PLAYER, 5000)
			;恋慕かつ相手が主人
			ELSEIF TALENT:ARG:恋慕
				PRINTFORML %CALLNAME:ARG%は%CALLNAME:MASTER%に処女を捧げられた事に幸福を感じているようだ……
				CALL SOURCE_CALC_歓楽(ARG, PLAYER, 2000)
			ENDIF
		ENDIF
	ELSE
		IF TALENT:ARG:貞操 > 0 && ARG:1 >= 0
			;非恋慕
			IF DT_SELECT("関係性データベース", @"(対象キャラ１ = {CFLAG:ARG:人物番号} Or 対象キャラ２ = {CFLAG:ARG:人物番号}) And (対象キャラ１ = {CFLAG:(ARG:1):人物番号} Or 対象キャラ２ = {CFLAG:(ARG:1):人物番号}) And 関係性種別 = '恋慕'") == 0
				CALL SOURCE_CALC_不潔(ARG, ARG:1, 1000)
			;チンコじゃない
			ELSEIF ARG:2 != 1
				CALL SOURCE_CALC_不潔(ARG, ARG:1, 500)
			ENDIF
		ELSEIF TALENT:ARG:貞操 == 0 && ARG:1 >= 0
			;非恋慕
			IF DT_SELECT("関係性データベース", @"(対象キャラ１ = {CFLAG:ARG:人物番号} Or 対象キャラ２ = {CFLAG:ARG:人物番号}) And (対象キャラ１ = {CFLAG:(ARG:1):人物番号} Or 対象キャラ２ = {CFLAG:(ARG:1):人物番号}) And 関係性種別 = '恋慕'") == 0
				CALL SOURCE_CALC_不潔(ARG, ARG:1, 500)
			;チンコじゃない
			ELSEIF ARG:2 != 1
				CALL SOURCE_CALC_不潔(ARG, ARG:1, 250)
			ENDIF
		ENDIF
	ENDIF
