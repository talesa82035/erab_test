					RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/リーリエ_ルザミーネ/リーリエ_ルザミーネ_並んで立つ2
				CASE 3
					RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/リーリエ_ルザミーネ/リーリエ_ルザミーネ_並んで立つ3
				CASE 4
					RESULTS = %CSTR:(指定キャラ:0):画像フォルダ%/リーリエ_ルザミーネ/リーリエ_ルザミーネ_並んで立つ4
				ENDSELECT
			ENDIF
		ENDIF
ENDSELECT

